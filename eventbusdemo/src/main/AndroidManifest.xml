<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.promise.eventbusdemo"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".eventbus.EventBusActivity">
        </activity>
        <activity android:name=".eventbus.EventPostActivity">
        </activity>
        <activity android:name=".otto.OttoReceiverActivity">
        </activity>
        <activity android:name=".otto.OttoPostActivity">
        </activity>
        <activity android:name=".otto.ProduceActivity">
        </activity>
        <activity android:name=".otto.SecondPageActivity">
        </activity>
        <activity android:name=".otto.FirstPageActivity">
        </activity>
    </application>

</manifest>